{
  "bindings": {
    "test0": {
      "type": "http-filesystem",
      "kind": "proxy",
      "routes": [
        {
          "when": [
            {
              "path": "/"
            }
          ],
          "exit": "filesystem_server0",
          "with": {
            "path": "index.html"
          }
        }
      ]
    }
  }
}